<View>
    <Header value="Text content"/>
    <Text name="title" value="$text_1" selectionEnabled="false"/>
    <!--<HyperText name="video" width="540" height="auto" value="$video_embedd"/>-->

    <HyperText name="gap1" value="">
        <br/>
    </HyperText>
    <Header value="Media content"/>
    <View style="display: flex; flex-wrap: wrap;">
        <View>
            <Text value="Media 1" name="media_1_l"/>
            <Image width="640px" valueList="$img_1" name="img_1" zoomControl="false"/>
        </View>
        <View>
            <Text value="Media 2" name="media_2_l"/>
            <Image width="640px" valueList="$img_2" name="img_2" zoomControl="false"/>
        </View>
        <View>
            <Text value="Media 3" name="media_3_l"/>
            <Image width="640px" valueList="$img_3" name="img_3" zoomControl="false"/>
        </View>
        <View>
            <Text value="Media 4" name="media_4_l"/>
            <Image width="640px" valueList="$img_4" name="img_4" zoomControl="false"/>
        </View>
    </View>

    <Header value="Is there any harmful content?"/>
    <Choices name="any_harmful" toName="title" showInline="true" choice="multiple">
        <Choice value="Text"/>
        <Choice value="Media 1"/>
        <Choice value="Media 2"/>
        <Choice value="Media 3"/>
        <Choice value="Media 4"/>
    </Choices>

    <View>
        <Header value="Is there Nature?"/>
        <Choices name="any_nature" toName="title">
            <Choice value="Text No / Media No" alias="-"/>
            <Choice value="Text Yes / Media No" alias="t"/>
            <Choice value="Text No / Media Yes" alias="m"/>
            <Choice value="Text Yes/ Media Yes" alias="tm"/>
        </Choices>
    </View>

    <View visibleWhen="choice-selected" whenTagName="any_nature" whenChoiceValue="t,m,tm">

        <Header value="Is there a Stewardship Action?"/>
        <Choices name="any_stewardship" toName="title">
            <Choice value="Text No / Media No" alias="-"/>
            <Choice value="Text Yes / Media No" alias="t"/>
            <Choice value="Text No / Media Yes" alias="m"/>
            <Choice value="Text Yes/ Media Yes" alias="tm"/>
        </Choices>

        <Header value="Is there any Nature Value Expression (either in the text or in the media content)?"/>
        <Choices name="any_value_expression" toName="title" showInline="true">
            <Choice value="No"/>
            <Choice value="Yes"/>
        </Choices>

        <View visibleWhen="choice-selected" whenTagName="any_value_expression" whenChoiceValue="Yes" required="true">
            <Header value="Nature Value Expression in Text"/>
            <Choices name="text_value_expression" toName="title" showInline="true">
                <Choice value="None"/>
                <Choice value="Implicit"/>
                <Choice value="Explicit"/>
            </Choices>

            <Header value="Nature Value Expression in Media content"/>
            <Choices name="media_value_expression" toName="title" required="true" showInline="true">
                <Choice value="None"/>
                <Choice value="Implicit"/>
                <Choice value="Explicit"/>
            </Choices>

            <Header value="Relevant Media"/>
            <Text name="rel_media" value="Select relevant Media items" selectionEnabled="false"/>
